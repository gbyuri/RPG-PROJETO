      escreval("                                                               |     |       |[  === ] /|   .:::::;;;::::::::;;;:::::.   |\ [=  ===] |       |")
      escreval("                                                               |_____|_______|[== = =]/ |  .:::::;;;::::::::::;;;:::::.  | \[ ==  =]_|______ |")
      escreval("                                                               | |       |    [ == = ] /| .::::::;;:::::::::::;;;::::::. |\ [== == ]      |  |")
      escreval("                                                               | |       |    [ == = ] /| .::::::;;:::::::::::;;;::::::. |\ [== == ]      |  |")
      escreval("                                                               |   |       |  [ === =] /.::::::;;::::::::::::::;;;:::::::.\ [===  =]   |     |")
      escreval("                                                               |___|_______|__[ == ==]/.::::::;;;:::::::::::::::;;;:::::::.\[=  == ]___|_____|")
      escreva("                                                               |                                   NOME> ",NOMEJOGADOR)
      N <- N - TAMANHONOME
      PARA J DE 1 ATE N FACA
         ESCREVA(" ")

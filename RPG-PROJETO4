///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
PROCEDIMENTO CRIARPERSONAGEM
VAR
   X,Y,J:INTEIRO
INICIO
   N <- 30
   C <- 30
   SE HPJOGADOR > 9 ENTAO
      H <- 17
   SENAO
      H<-18
   FIMSE
   SE DEFJOGADOR > 9 ENTAO
      EX <- 14
   SENAO
      EX <- 15
   FIMSE
   escreval("                                                                          ___________________________________________________")
   escreval("                                                                         |           /\                                      |")
   escreval("                                                                         | _         )( ______________________               |")
   escreval("                                                                         |(_)/'/'/'/(**)______________________>              |")
   escreva("                                                                         |           )(   NOME:",NOMEJOGADOR)
   N <- N - TAMANHONOME
   PARA J DE 1 ATE N FACA
      ESCREVA(" ")
   FIMPARA
   ESCREVAL("|")
   escreva("                                                                         |           \/ CLASSE:",CLASSEJOGADOR)
   C <- C - TAMANHOCLASSE
   PARA J DE 1 ATE C FACA
      ESCREVA(" ")
   FIMPARA
   ESCREVAL("|")
   escreval("                                                                         |                                                   |")
   escreva("                                                                         |                 FOR:",FOR,"     HP:",HPJOGADOR)
   PARA J DE 1 ATE H FACA
      ESCREVA(" ")
   FIMPARA
   ESCREVAL("|")
   escreval("                                                                         |                 DES:",DES,"     ATK:",ATKJOGADOR,"                 |")
   escreval("                                                                         |                 INT:",INTE,"    <DANO>",DANOMIN:2:0," ~",DANOMAX:2:0,"            |")
   escreval("                                                                         |                                                   |")
   escreva("                                                                         |               ACERTO:",ACERTO,"      DEF:",DEFJOGADOR)
   PARA J DE 1 ATE EX FACA
      ESCREVA(" ")
   FIMPARA
   ESCREVAL("|")
   escreval("                                                                         |                                      /\           |")
   escreval("                                                                         |               ______________________ )(         _ |")
   escreval("                                                                         |              <______________________(**)\'\'\'\(_)|")
   escreval("                                                                         |                                      )(           |")
   escreval("                                                                         |                                      \/           |")
   escreval("                                                                         |___________________________________________________|")
   SE NOMEJOGADOR = "" ENTAO

      ESCREVAL("                                                                                         < DIGA SEU NOME >")
      ESCREVA("                                                                                           <")
      LEIA(NOMEJOGADOR)
      TAMANHONOME <- COMPR(NOMEJOGADOR)
      LIMPATELA
      CRIARPERSONAGEM

   SENAO
   SE (NOMEJOGADOR <> "") E (CLASSEJOGADOR = "") ENTAO

      ANIMACAOCLASSE

      ESCREVAL("                                                                                        < ESCOLHA SUA CLASSE >")
      ESCREVA("                                                                                                   ")
      LEIA(Y)
      ESCOLHA Y
      CASO 1
         CLASSEJOGADOR <- "GUERREIRO"
         GUERREIRO <- VERDADEIRO
         FOR<- 3
         DES<- 1
         INTE<-1
         HPJOGADOR<-25 +(5*FOR)
         HPMAXJOGADOR <- HPJOGADOR
         ATKJOGADOR<-5 + FOR
         DANOMIN<-(FOR+ATKJOGADOR)/2
         DANOMAX<-(FOR+ATKJOGADOR)
         ACERTO<-1 + DES
         DEFJOGADOR<-15 + FOR
         EXPJOGADOR <- 0
         EXPMAXJOGADOR <- 10
         LIMPATELA
         TAMANHOCLASSE <- COMPR(CLASSEJOGADOR)
         CRIARPERSONAGEM

      CASO 2
         CLASSEJOGADOR <-"ARQUEIRO"
         ARQUEIRO <- VERDADEIRO
         FOR<- 1
         DES<- 3
         INTE<-1
         HPJOGADOR<-20 +(5*FOR)
         HPMAXJOGADOR <- HPJOGADOR
         ATKJOGADOR<-5 + DES
         DANOMIN<-(DES+ATKJOGADOR)/2
         DANOMAX<-(DES+ATKJOGADOR)
         ACERTO<-1 + DES
         DEFJOGADOR<-10 + FOR
         EXPJOGADOR <- 0
         EXPMAXJOGADOR <- 10
         LIMPATELA
         TAMANHOCLASSE <- COMPR(CLASSEJOGADOR)
         CRIARPERSONAGEM


      CASO 3
         CLASSEJOGADOR <- "MAGO"
         MAGO <- VERDADEIRO
         FOR<- 1
         DES<- 1
         INTE<-3
         HPJOGADOR<-15 +(5*FOR)
         HPMAXJOGADOR <- HPJOGADOR
         ATKJOGADOR<-5 + INTE
         DANOMIN<-(INTE+ATKJOGADOR)/2
         DANOMAX<-(INTE+ATKJOGADOR)
         ACERTO<-1 + DES
         DEFJOGADOR<-5 + FOR
         EXPJOGADOR <- 0
         EXPMAXJOGADOR <- 10
         LIMPATELA
         TAMANHOCLASSE <- COMPR(CLASSEJOGADOR)
         CRIARPERSONAGEM
      FIMESCOLHA
   SENAO
   SE (NOMEJOGADOR <> "") E (CLASSEJOGADOR <> "") ENTAO
      LIVRO3
      ESCREVAL("                                                                                           < [1] OU [2] >")
      ESCREVA("                                                                                                 ")
      LEIA(X)
      ESCOLHA X
      CASO 1
      HUB <- VERDADEIRO
      CASO 2
         NOMEJOGADOR <- ""
         TAMANHONOME <- 0
         CLASSEJOGADOR <- ""
         TAMANHOCLASSE <- 0
         GUERREIRO <- FALSO
         ARQUEIRO <- FALSO
         MAGO <- FALSO
         FOR<- 0
         DES<- 0
         INTE<-0
         HPJOGADOR<-0 +(5*FOR)
         HPMAXJOGADOR <- HPJOGADOR
         ATKJOGADOR<-0 + INTE
         DANOMIN<-(INTE+ATKJOGADOR)/2
         DANOMAX<-(INTE+ATKJOGADOR)
         ACERTO<-0 + DES
         DEFJOGADOR<-0 + FOR
         EXPJOGADOR <- 0
         EXPMAXJOGADOR <- 0
         LIMPATELA
         ANIMACAOX
         CRIARPERSONAGEM
      FIMESCOLHA
   FIMSE
   FIMSE
   FIMSE

FIMPROCEDIMENTO

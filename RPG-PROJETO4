///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
Inicio
   //   AVISO
   //   ESCREVA("                                                                                                ")
   //   LEIA(CONTINUAR)
   //   TORRE
   TIMER 1000
   TIMER 0
   LIMPATELA
   CONTINUAR <- ""
   I <- VERDADEIRO
   ENQUANTO I = VERDADEIRO FACA
      ENQUANTO (ESCOLHAMENUPERSONAGEM <> 1) FACA
         LIVRO1
         ESCREVAL("                                                                                           < [1] OU [2] >")
         ESCREVA("                                                                                                 ")
         LEIA(ESCOLHAMENUPERSONAGEM)
         SE (ESCOLHAMENUPERSONAGEM = 1) ENTAO
            ANIMACAO
            CRIARPERSONAGEM
            ANIMACAOY
         FIMSE
         SE (ESCOLHAMENUPERSONAGEM = 2) ENTAO
            I <- FALSO
            LIMPATELA
         FIMSE
         LIMPATELA
      FIMENQUANTO
      //-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
      ENQUANTO HUB = VERDADEIRO FACA
      N <- 36
      C <- 36
      SE HPJOGADOR > 9 ENTAO
         H <- 29
      SENAO
         H <-30
      FIMSE
      SE EXPJOGADOR >9 ENTAO
         EX <- 29
      SENAO
         EX <- 30
      FIMSE
